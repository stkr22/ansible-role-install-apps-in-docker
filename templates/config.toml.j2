concurrent = 1
check_interval = 0

[session_server]
    session_timeout = 1800

[[runners]]
    name = "{{ ansible_hostname }}: docker"
    url = "{{ gitlab_instance_url }}"
    token = "{{ gitlab_runner_token }}"
    executor = "docker"
    [runners.docker]
        tls_verify = false
        image = "python:3.9"
        privileged = false
        disable_entrypoint_overwrite = false
        oom_kill_disable = false
        disable_cache = false
        volumes = ["/cache"]
        shm_size = 0
    [runners.custom_build_dir]
    [runners.cache]
        [runners.cache.s3]
        [runners.cache.gcs]
        [runners.cache.azure]